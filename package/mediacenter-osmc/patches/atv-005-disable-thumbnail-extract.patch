From 71fa80c1d947678424403f75bb772a876ae54ac8 Mon Sep 17 00:00:00 2001
From: Sam Nazarko <email@samnazarko.co.uk>
Date: Tue, 2 Jun 2015 22:16:09 +0100
Subject: [PATCH] Disable thumbnail extraction by default on low power
 platforms

Signed-off-by: Sam Nazarko <email@samnazarko.co.uk>
---
 system/settings/settings.xml | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/system/settings/settings.xml b/system/settings/settings.xml
index d1acdae..40ec104 100644
--- a/system/settings/settings.xml
+++ b/system/settings/settings.xml
@@ -863,7 +863,7 @@
         </setting>
         <setting id="myvideos.extractthumb" type="boolean" label="38103" help="36180">
           <level>1</level>
-          <default>true</default>
+          <default>false</default>
           <control type="toggle" />
         </setting>
       </group>
