# table imon_pad, type: RC6
0x0100007f KEY_RIGHT
0x01000080 KEY_LEFT
0x01007f00 KEY_DOWN
0x01008000 KEY_UP
0x0200001E KEY_1
0x0200001e KEY_1
0x0200001f KEY_2
