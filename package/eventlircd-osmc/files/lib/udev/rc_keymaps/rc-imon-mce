0x02000029 KEY_ESC
0x0200002a KEY_EXIT
0x0200002A KEY_PREVIOUS
0x0200002C KEY_SETUP
0x0200004f KEY_RIGHT
0x02000050 KEY_LEFT
0x02000051 KEY_DOWN
0x02000052 KEY_UP
0x02000065 KEY_PROG2
0x02200020 KEY_IMAGES
0x02200025 KEY_VIDEO
0x02800000 KEY_PROG1
